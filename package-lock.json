{
  "name": "ticketing-monorepo",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "ticketing-monorepo",
      "workspaces": [
        "services/api",
        "services/web"
      ]
    },
    "node_modules/ticketing-api": {
      "resolved": "services/api",
