    "node_modules/ticketing-web": {
      "resolved": "services/web",
      "link": true
    },
    "services/api": {
      "name": "ticketing-api",
      "version": "0.1.0",
      "devDependencies": {
        "nodemon": "^3.1.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3"
      }
    },
    "services/web": {
      "name": "ticketing-web",
      "version": "0.1.0"
    }
  }
}
