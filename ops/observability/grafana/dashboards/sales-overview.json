{
  "title": "Ticketing â€” Sales Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Paid Orders",
      "targets": [{ "expr": "ticketing_paid_orders_total", "refId": "A" }],
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 }
    },
    {
      "type": "stat",
      "title": "Revenue (RUB)",
      "targets": [{ "expr": "ticketing_revenue_cents_total / 100", "refId": "A" }],
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 }
    },
    {
      "type": "timeseries",
      "title": "API latency p95 (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 }
    }
  ]
}
